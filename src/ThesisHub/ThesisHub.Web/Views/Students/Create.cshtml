@model StudentViewModel

@{
    Layout = "_Create";
    ViewData["Entity"] = "Student";
}

@section Fields {
    @await Html.PartialAsync("_DynamicFields", Model.DynamicFields)
    @await Html.PartialAsync("_SelectField", Model.StaticFields.Last())
}

@section AdditionalScripts {
    <script>
        loadSelect();
    </script>
}