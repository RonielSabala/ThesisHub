@model StudentViewModel

@{
    Layout = "GenericViews/_Edit";
    ViewData["Entity"] = "Student";
    ViewData["ModelId"] = Model.Id;
}

@section Fields {
    @await Html.PartialAsync("Fields/_DynamicFields", Model.DynamicFields)
    @await Html.PartialAsync("Fields/_Select", Model.StaticFields.Last())
}